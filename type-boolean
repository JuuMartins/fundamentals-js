const firstNumber = 5;
const secondNumber = 10;

console.log(firstNumber === secondNumber); // false

const firstNum = 10;
const secondNum = 10;

console.log(firstNum === secondNum); // true

const text1 = "Alura";
const text2 = "A";

console.log(text1 === text2); // false

const text3 = "Alura";
const text4 = "alura";

console.log(text3 === text4); // false